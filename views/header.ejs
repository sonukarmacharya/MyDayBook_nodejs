        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  
    </head>
    <body>
        <form method="POST" action="/dash">
              <nav class="navbar navbar-inverse">
                    <div class="container-fluid">
                      <ul class="nav navbar-nav">
                        <li ><a href="/dash">logo</a></li>
                        
                      </ul>
                      <div class="navbar-header">
                        <a class="navbar-brand" href="/dash">WebSiteName</a>
                      </div>
                      
                      <ul class="nav navbar-nav navbar-right">
                        <li> <a  href="/addDay">Add Day</a>
                        </li>
                        <li>
                         <form  method="POST" action="/search">
                          <div style="margin-top: 3%;margin-left: 4%;">
                          <input type="text" id="searchTitle" class="form-control" placeholder="Search form title" name="searchformTitle">
                          </div></li>
                          <li>
                            <input type="submit" class="btn btn-default" style="margin-top: 7%;margin-left:5%;" value="Search">
                        </form>
                    
                        </li>
                        <li><a href="/logout"><span class="glyphicon glyphicon-log-in"></span> Logout</a></li>
                      </ul>
                    </div>
                    <script>
                      $(function(){
                     $('#searchTitle').autocomplete({
                       source: function(req,res){
                         $.ajax({
                           url:"autocomplete",
                           dataType:"jsonp",
                           type:"GET",
                           data:req,
                           success:  function(data){
                           res(data)

                           },
                           error: function(err){ console.log(err.status)},
                         });
                       },
                       minLength:1,
                       select: function(event,ui){
                         if(ui.item){
                           $('#searchTitle').text(ui.item.label)
                         }
                       }
                     })
                      })
                    </script>
                  </nav>
        </form>
        </body>
